{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "node",
    "module": "commonjs",
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "declaration": false,
    "pretty": true,
    "lib": [
      "es6"
    ],
    "baseUrl": "."
  },
  "exclude": [
    "node_modules/**",
    "**/*.aot.ts"
  ]
}